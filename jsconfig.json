{
    "compilerOptions": {
        "module": "ES2022",
        "target": "ES2022",
        "checkJs": true,
        "strict": true,
        "strictNullChecks": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true,
        "suppressImplicitAnyIndexErrors": false,
    },
    "exclude": [
        "node_modules", 
        "**/node_modules/*"
    ]
}
  